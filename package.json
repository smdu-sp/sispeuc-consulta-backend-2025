    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "fs": "^0.0.1-security",
    "json-bigint": "^1.0.0",
    "ldapts": "^7.3.1",
    "multer": "^1.4.5-lts.1",
